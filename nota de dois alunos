#include<stdio.h>
int main(){
int n;
double maior=0;
scanf("%d", &n);

for (int i =0; i<n; i++);{
double nota;
scanf("%lf", & nota);
if(nota>maior){
    maior=nota;
}


}

printf("%.2lf|\n",maior);
return  0;
}

//comentarios
// escreva = printf
//leia = scanf
//antes de numero inteiro = coloque %d
//antes de numero real = coloque %lf
//ponto e virgula ao final de toda linha = ;
// inteiro = int
// real = double
// & = identifica ao ler
// escreva = printf
//leia = scanf
//antes de numero inteiro = coloque %d
//antes de numero real = coloque %lf
//ponto e virgula ao final de toda linha = ;
// inteiro = int
// real = double
// & = identifica ao ler
